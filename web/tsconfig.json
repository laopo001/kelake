{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "baseUrl": "src",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": false,
    "resolveJsonModule": true,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "lib": [
      "webworker",
      "dom",
      "es2015",
      "es2017",
      "es2016",
      "es2018",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "newLine": "lf",
    "outDir": "build/output",
    "paths": {
     
    },
    "plugins": [
      {
        "configFile": "./tslint.json",
        "name": "typescript-tslint-plugin"
      }
    ],
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "alwaysStrict": false,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ],
    "types": [
      "node"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src",
    "typings"
  ]
}
